{
  "name": "@pearson-components/coach-mark",
  "version": "0.4.0",
  "description": "Simple popover to explain new features to users.",
  "main": "./build/dist.coach-mark.js",
  "scripts": {
    "dev-setup": "mkdir -p fonts && mkdir -p icons && cp ./node_modules/pearson-elements/dist/fonts/* ./fonts && cp ./node_modules/pearson-elements/dist/icons/* ./icons",
    "dev": "npm run dev-setup && webpack-dev-server --port 8081 --devtool source-map --hot --progress --colors",
    "pretest": "npm run lint",
    "test": "./node_modules/karma/bin/karma start karma.conf.js",
    "build": "webpack -p; node ./npm_scripts/copyStuff.js",
    "lint": "eslint src/js/** demo/demo.js",
    "gen-changelog": "node ./npm_scripts/gen-changelog.js",
    "version": "npm run gen-changelog && git add CHANGELOG.md",
    "ci": "npm test && npm run build",
    "release": "npm run ci && node ./npm_scripts/release.js",
    "verify": "node ./npm_scripts/verify.js"
  },
  "author": "AegisTeam aegisteam@pearson.com",
  "license": "PEARSON PROPRIETARY AND CONFIDENTIAL INFORMATION SUBJECT TO NDA",
