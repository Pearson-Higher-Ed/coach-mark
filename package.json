    "css-loader": "~0.23.0",
    "eslint": "~1.10.3",
    "eslint-loader": "~1.1.1",
    "eslint-plugin-react": "~3.16.1",
    "expect": "~1.18.0",
    "expect-jsx": "~2.5.1",
    "file-loader": "~0.8.5",
    "fs-extra": "^4.0.2",
